{
  "name": "justexp",
  "packageManager": "yarn@4.1.1",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/*.js --recursive",
    "start": "node src/index.js",
    "test": "jest"
  },
  "nodemonConfig": {
    "delay": 1000,
    "ignore": [
      "*.test.js"
    ],
    "env": {
      "PORT": 5000,
      "DEBUG": "app:*",
      "NODE_ENV": "development"
    }
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "mongoose": "^8.3.3",
    "morgan": "^1.10.0",
    "nunjucks": "^3.2.4",
    "paseto": "^3.1.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.5"
  }
}
